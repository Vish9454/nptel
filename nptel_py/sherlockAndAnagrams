def sherlockAndAnagrams(s):
        res=0
        n=len(s)
        for i in range(1,n):
            d={}
            for j in range(n-i+1):
                subs=''.join(sorted(s[j:j+i]))
                if subs not in d:
                    d[subs]=1
                else:
                    d[subs]+=1
                res+=d[subs]-1
                print(subs,"=",res)

        print(res)

sherlockAndAnagrams("abba")


a = 0
b = 0
b = 1
a = 2
ab = 2
bb = 2
ab = 3
abb = 3
abb = 4
4
